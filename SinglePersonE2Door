@outputs Fade

    interval(250)
    
    if(first()){
        setName(owner():name() + "'s Single Person Door")
    }
    
    findInSphere(entity():pos(),25)
    findClipToClass("player")
    
    if(findResult(1) == owner()){
        Fade = 1
    }else{
        Fade = 0
    }

